<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.hallowebsite.setting">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_settings"
        android:label="@string/setting_name"
        android:name=".App"
        android:roundIcon="@drawable/ic_settings"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name=".SettingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".phoneinfo.AboutPhoneInfoActivity" />
        <activity android:name=".fundebug.FunDebugListActivity" />
        <activity android:name=".fundebug.filemanager.FileManagerActivity" />
        <activity android:name=".fundebug.showdialog.DialogShowActivity" />

    </application>
</manifest>